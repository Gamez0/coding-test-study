    "n = int(input())\n",
    "d = [0]*(n+1)\n",
    "square = [i*i for i in range(1,317)]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 19,
   "metadata": {},
   "outputs": [],
   "source": [
    "for i in range(1,n+1):\n",
    "    s=[]\n",
    "    for j in square:\n",
    "        if j>i:\n",
    "            break\n",
    "        s.append(d[i-j])\n",
    "    d[i]=min(s)+1"
   ]
