    "    n=int(input())\n",
    "    if n==0:\n",
    "        break\n",
    "    r=1\n",
    "    for n in range(2*n,n,-1):\n",
    "        r*=n\n",
    "    for n in range(1,n+1):\n",
    "        r//=n\n",
    "    print(r)"
