   "cell_type": "code",
   "execution_count": 14,
   "metadata": {},
   "outputs": [],
   "source": [
    "for i in range(2,n+1):\n",
    "    for j in range(10):\n",
    "        for k in range(j+1):\n",
    "            d[i][j]+=d[i-1][k]\n",
    "        d[i][j]%=10007"
   ]
  },
