   "source": [
    "n = int(input())\n",
    "d = [0 for _ in range(n+1)]\n",
    "if n!=1:\n",
    "    d[2]=3"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 27,
   "metadata": {},
   "outputs": [],
   "source": [
    "for i in range(4,n+1,2):\n",
    "    d[i]=3*d[i-2]\n",
    "    for j in range(4,i,2):\n",
    "        d[i]+=2*d[i-j]\n",
    "    d[i]+=2"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 28,
