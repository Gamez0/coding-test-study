   "source": [
    "n = int(input())\n",
    "s,op,count=[],[],1\n",
    "for i in range(n):\n",
    "    num = int(input())\n",
    "    while(count<=num):\n",
    "        s.append(count)\n",
    "        op.append('+')\n",
    "        count+=1\n",
    "    if s[-1]==num:\n",
    "        s.pop()\n",
    "        op.append('-')\n",
    "    else:\n",
    "        print(\"NO\")\n",
    "print('\\n'.join(op))"
   ]
